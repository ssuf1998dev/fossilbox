{
  "name": "fossilbox",
  "type": "module",
  "version": "0.1.0",
  "private": true,
  "packageManager": "pnpm@9.10.0",
  "description": "Yet another but minimalist web archiving service.",
  "author": "ssuf1998dev <ssuf1998dev@126.com>",
  "license": "MIT",
  "keywords": [],
  "engines": {
    "node": ">=18"
  },
  "scripts": {
    "prepare": "husky",
    "dev": "pnpm vn --watch server/index.ts",
    "start": "node dist",
    "build:app": "remix vite:build",
    "build:server": "rollup -c",
    "build": "pnpm run --sequential \"/^build:.*/\"",
    "vn": "vite-node --config vite-node.config.ts"
  },
  "dependencies": {
    "@libsql/client": "^0.14.0",
    "@remix-run/express": "^2.13.1",
    "@remix-run/node": "^2.13.1",
    "@remix-run/react": "^2.13.1",
    "c12": "^2.0.1",
    "consola": "^3.2.3",
    "drizzle-kit": "^0.26.2",
    "drizzle-orm": "^0.35.1",
    "execa": "^9.4.1",
    "express": "^4.21.1",
    "isbot": "4",
    "lodash-es": "^4.17.21",
    "puppeteer-core": "^23.5.3",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "remix-auth": "^3.7.0",
    "remix-auth-form": "^1.5.0"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^3.7.3",
    "@commitlint/cli": "^19.5.0",
    "@commitlint/config-conventional": "^19.5.0",
    "@remix-run/dev": "^2.13.1",
    "@rollup/plugin-commonjs": "^28.0.1",
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-node-resolve": "^15.3.0",
    "@rollup/plugin-replace": "^6.0.1",
    "@rollup/plugin-typescript": "^12.1.1",
    "@types/express": "^5.0.0",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^22.7.5",
    "@types/react": "^18.3.11",
    "commitizen": "^4.3.1",
    "cross-env": "^7.0.3",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^9.12.0",
    "eslint-plugin-format": "^0.1.2",
    "husky": "^9.1.6",
    "lint-staged": "^15.2.10",
    "magic-string": "^0.30.12",
    "rollup": "^4.24.0",
    "tslib": "^2.8.0",
    "typescript": "5.6.3",
    "vite": "^5.4.9",
    "vite-node": "^2.1.3",
    "vite-tsconfig-paths": "^5.0.1"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "lint-staged": {
    "*.{ts,js,cjs,tsx,jsx}": "eslint --cache"
  }
}
